<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="shz_stamp_tax_2_euro" model="product.product">
        <field name="name">Timbre fiscal</field>
        <field name="type">service</field>
        <field name="list_price" eval="0.0"/>
        <field name="standard_price" eval="0.0"/>
        <field name="description_sale">Timbre fiscal</field>
        <field name="is_stamp" eval="True"/>
        <field name="auto_compute" eval="True"/>
        <field name="stamp_apply_min_total_base" eval="0.0"/>
        <field name="taxes_id" eval="[(6, 0, [])]"/>
        <field name="supplier_taxes_id" eval="[(6, 0, [])]"/>
    </record>

    <record id="base.main_company" model="res.company">
        <field
            name="tax_stamp_product_id"
            ref="shz_stamp_tax.shz_stamp_tax_2_euro"
        />
    </record>

</odoo>
